<?xml version="1.0" encoding="UTF-8"?>
<jcr:root xmlns:sling="http://sling.apache.org/jcr/sling/1.0" xmlns:cq="http://www.day.com/jcr/cq/1.0" xmlns:jcr="http://www.jcp.org/jcr/1.0" xmlns:nt="http://www.jcp.org/jcr/nt/1.0"
    jcr:primaryType="cq:Page">
    <jcr:content
        cq:template="/conf/asset-share-commons/settings/wcm/templates/action-template"
        jcr:primaryType="cq:PageContent"
        jcr:title="Download"
        sling:resourceType="asset-share-commons/components/structure/page">
        <root
            jcr:primaryType="nt:unstructured"
            sling:resourceType="wcm/foundation/components/responsivegrid">
            <responsivegrid
                jcr:primaryType="nt:unstructured"
                sling:resourceType="wcm/foundation/components/responsivegrid">
                <download
                    jcr:primaryType="nt:unstructured"
                    sling:resourceType="asset-share-commons/components/modals/download"
                    assetListTitle="Assets"
                    cancelButton="Cancel"
                    downloadButton="Download"
                    downloadOptionsTitle="Download options"
                    fileName="Assets"
                    maxContentSizeMessage="Maximum content size of download exceeded."
                    modalTitle="Download"
                    noContentFileName="NO DOWNLOADABLE RENDITIONS.txt"
                    noContentMessage="Sorry, we could not find any downloadable files for the selected assets/renditions combinations."
                    selectAllLabel="Select all">
                    <asset-renditions-groups jcr:primaryType="nt:unstructured">
                        <items jcr:primaryType="nt:unstructured">
                            <item0
                                jcr:primaryType="nt:unstructured"
                                assetRenditionsGroupTitle="All">
                                <asset-renditions jcr:primaryType="nt:unstructured">
                                    <items jcr:primaryType="nt:unstructured">
                                        <item0
                                            jcr:primaryType="nt:unstructured"
                                            selected="false"
                                            text="Original"
                                            value="original"/>
                                    </items>
                                </asset-renditions>
                            </item0>
                            <item1
                                jcr:primaryType="nt:unstructured"
                                assetRenditionsGroupTitle="Images">
                                <asset-renditions jcr:primaryType="nt:unstructured">
                                    <items jcr:primaryType="nt:unstructured">
                                        <item0
                                            jcr:primaryType="nt:unstructured"
                                            selected="false"
                                            text="Web"
                                            value="web"/>
                                        <item1
                                            jcr:primaryType="nt:unstructured"
                                            selected="false"
                                            text="Smart Crop (Small)"
                                            value="smart-crop-small"/>
                                    </items>
                                </asset-renditions>
                            </item1>
                            <item2
                                jcr:primaryType="nt:unstructured"
                                assetRenditionsGroupTitle="Videos">
                                <asset-renditions jcr:primaryType="nt:unstructured">
                                    <items jcr:primaryType="nt:unstructured">
                                        <item0
                                            jcr:primaryType="nt:unstructured"
                                            selected="false"
                                            text="MP4"
                                            value="mp4"/>
                                        <item1
                                            jcr:primaryType="nt:unstructured"
                                            selected="false"
                                            text="M4V"
                                            value="m4v"/>
                                    </items>
                                </asset-renditions>
                            </item2>
                        </items>
                    </asset-renditions-groups>
                </download>
            </responsivegrid>
        </root>
    </jcr:content>
</jcr:root>
